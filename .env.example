# EmailJS Configuration
# Get these from https://emailjs.com/ after setting up your account

# Main service ID from EmailJS dashboard
VITE_EMAILJS_SERVICE_ID=your_service_id_here

# Template ID for main emails (sent to you)
VITE_EMAILJS_TEMPLATE_ID=your_template_id_here

# Template ID for auto-reply emails (sent to visitors)
VITE_EMAILJS_AUTOREPLY_TEMPLATE_ID=your_autoreply_template_id_here

# Public key from EmailJS account settings
VITE_EMAILJS_PUBLIC_KEY=your_public_key_here
